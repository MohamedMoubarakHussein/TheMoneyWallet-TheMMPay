<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://themoneywallet.com/transactionservice/common"
           xmlns:tns="http://themoneywallet.com/transactionservice/common"
           elementFormDefault="qualified">

    <xs:simpleType name="TransactionStatus">
        <xs:restriction base="xs:string">
            <xs:enumeration value="PENDING"/>
            <xs:enumeration value="INITIATED"/>
            <xs:enumeration value="PROCESSING"/>
            <xs:enumeration value="COMPLETED"/>
            <xs:enumeration value="FAILED"/>
            <xs:enumeration value="REVERSED"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>